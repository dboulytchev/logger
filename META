version = "0.4.4"
description = "Logger as a Camlp5 syntax extension"

package "syntax" (
 description = "Syntax extension for Logger"
 requires = "camlp5 camlp5.pr_o"

 archive(syntax,preprocessor) = "pa_log.cmo"
 archive(syntax,toploop)      = "pa_log.cmo"
 archive(syntax,native,preprocessor) = "pa_log.cmx"
 archive(syntax,native,toploop)      = "pa_log.cmx"
 
)

